{
  "name": "info",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/info",
  "projectType": "application",
  "implicitDependencies": ["ui"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["dist/apps/info"],
      "options": {
        "command": "sh ./scripts/build-info.sh"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "configurations": {
        "development": {
          "command": "pnpm astro dev",
          "cwd": "apps/info"
        },
        "production": {
          "command": "pnpm astro preview",
          "cwd": "apps/info"
        }
      }
    },
    "deploy": {
      "command": "sh ./scripts/deploy-info.sh"
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/info/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": ["runtime:browser"]
}
