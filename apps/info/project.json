{
  "name": "info",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/info",
  "projectType": "application",
  "implicitDependencies": ["ui"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["dist/apps/info"],
      "options": {
        "command": "sh ./scripts/things/build.sh"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "options": {
        "command": "pnpm astro dev",
        "cwd": "apps/info"
      }
    },
    "preview": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "info:build",
        "staticFilePath": "dist/apps/info"
      },
      "configurations": {
        "ci": {
          "buildTarget": "ui:build-storybook:ci"
        }
      }
    },
    "deploy": {
      "command": "sh ./scripts/things/deploy.sh"
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/info/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": ["runtime:browser"]
}
