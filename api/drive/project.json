{
  "name": "api-drive",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "api/drive/src",
  "targets": {
    "build": {
      "executor": "@nx/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/api/drive",
        "tsConfig": "api/drive/tsconfig.lib.json",
        "main": "api/drive/src/index.ts",
        "packageJson": "api/drive/package.json",
        "assets": ["api/drive/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["api/drive/**/*.ts"]
      }
    }
  },
  "tags": ["runtime:node", "lib:api"]
}
